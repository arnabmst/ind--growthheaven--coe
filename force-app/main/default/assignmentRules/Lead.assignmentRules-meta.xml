<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>limited Partner Lead assignment Rule</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>High_Worth_Lead_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Net_Worth__c</field>
                <operation>greaterOrEqual</operation>
                <value>80000</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Medium_Worth_Lead_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Net_Worth__c</field>
                <operation>greaterOrEqual</operation>
                <value>30000</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Net_Worth__c</field>
                <operation>lessThan</operation>
                <value>80000</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>General_Lead_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Net_Worth__c</field>
                <operation>lessThan</operation>
                <value>30000</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
